                  <script>
                    function validateForm() {
                      var x = document.forms["myForm"]["code"].value;
                      if (x == "") {
                        alert("Yêu cầu nhập mã sản phẩm");
                        return false;
                      }
                      else if (document.forms["myForm"]["nameProduct"].value == "") {
                        alert("Yêu cầu nhập tên sản phẩm");
                        return false;
                      }
                      else if (document.forms["myForm"]["category"].value == "") {
                        alert("Yêu cầu nhập thể loại sản phẩm");
                        return false;
                      }
                      else if (document.forms["myForm"]["price"].value == "") {
                        alert("Yêu cầu nhập giá sản phẩm");
                        return false;
                      }
                      else if (document.forms["myForm"]["quanlity"].value == "") {
                        alert("Yêu cầu nhập số lượng sản phẩm");
                        return false;
                      }
                      else if (document.forms["myForm"]["image"].value == "") {
                        alert("Yêu cầu nhập ảnh sản phẩm");
                        return false;
